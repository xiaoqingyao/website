tion  <%if(hsFieldValue["jobclass"] != null && hsFieldValue["jobclass"].ToString().Contains("2")) {Response.Write("selected='selected'");}%> value="2">平面设计</option><option  <%if(hsFieldValue["jobclass"] != null && hsFieldValue["jobclass"].ToString().Contains("3")) {Response.Write("selected='selected'");}%> value="3">网站制作</option><option  <%if(hsFieldValue["jobclass"] != null && hsFieldValue["jobclass"].ToString().Contains("4")) {Response.Write("selected='selected'");}%> value="4">销售</option><option  <%if(hsFieldValue["jobclass"] != null && hsFieldValue["jobclass"].ToString().Contains("5")) {Response.Write("selected='selected'");}%> value="5">网络推广</option><option  <%if(hsFieldValue["jobclass"] != null && hsFieldValue["jobclass"].ToString().Contains("6")) {Response.Write("selected='selected'");}%> value="6">财务</option><option  <%if(hsFieldValue["jobclass"] != null && hsFieldValue["jobclass"].ToString().Contains("7")) {Response.Write("selected='selected'");}%> value="7">其它</option></select></dd></dl><dl><dt>学历要求</dt><dd><select  id="requirements" name="requirements"><option  <%if(hsFieldValue["requirements"] != null && hsFieldValue["requirements"].ToString().Contains("0")) {Response.Write("selected='selected'");}%> value="0">不限</option><option  <%if(hsFieldValue["requirements"] != null && hsFieldValue["requirements"].ToString().Contains("1")) {Response.Write("selected='selected'");}%> value="1">高中</option><option  <%if(hsFieldValue["requirements"] != null && hsFieldValue["requirements"].ToString().Contains("2")) {Response.Write("selected='selected'");}%> value="2">专科</option><option  <%if(hsFieldValue["requirements"] != null && hsFieldValue["requirements"].ToString().Contains("3")) {Response.Write("selected='selected'");}%> value="3">本科</option><option  <%if(hsFieldValue["requirements"] != null && hsFieldValue["requirements"].ToString().Contains("4")) {Response.Write("selected='selected'");}%> value="4">硕士</option><option  <%if(hsFieldValue["requirements"] != null && h