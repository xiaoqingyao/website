<ul id="tags"><li class="selectTag"><a href="javascript:;">基本信息</a> </li><li><a href="javascript:;">联系方式</a> </li></ul><div id="panel"><fieldset><dl><dt>姓名</dt><dd><input  class="validate[required]"  type="text" value="<%=hsFieldValue["Name"]%>" style="width:180px;"  maxlength="128" name="Name" id="Name" /><span style="color:#ff0000; font-size:14px; font-weight:bold;">*</span></dd></dl><dl><dt>性别</dt><dd><select  id="Sex" name="Sex"><option  <%if(hsFieldValue["Sex"] != null && hsFieldValue["Sex"].ToString().Contains("1")) {Response.Write("selected='selected'");}%> value="1">男</option><option  <%if(hsFieldValue["Sex"] != null && hsFieldValue["Sex"].ToString().Contains("2")) {Response.Write("selected='selected'");}%> value="2">女</option></select></dd></dl><dl><dt>年龄</dt><dd><input  class="validate[optional],custom[onlyNumber],numberMinMax[1,200]"  type="text" value="<%=hsFieldValue["Age"]%>" style="width:180px;"  maxlength="10000" name="Age" id="Age" /></dd></dl><dl><dt>出生日期</dt><dd><input id="BirthDay"  value="<%if(hsFieldValue["BirthDay"].ToString().Equals("none")){Response.Write("");}else{Response.Write(string.Format("{0:yyyy-MM-dd}",hsFieldValue["BirthDay"]));}%>"   name="BirthDay" style="width:180px;"  class="Wdate" type="text" onFocus="WdatePicker({lang:'<%=Resources.Model.DateLang%>',skin:'whyGreen',dateFmt:'yyyy-MM-dd'})"/></dd></dl><dl><dt>婚姻状况</dt><dd><input  type="radio" <%if(hsFieldValue["IsMarried"] != null && hsFieldValue["IsMarried"].ToString().Contains("1")) {Response.Write("checked='checked'");}%>  name="IsMarried" value="1" />是<input  type="radio" <%if(hsFieldValue["IsMarried"] != null && hsFieldValue["IsMarried"].ToString().Contains("0")) {Response.Write("checked='checked'");}%>  name="IsMarried" value="0" />否</dd></dl><dl><dt>身高</dt><dd><input  class="validate[optional],custom[onlyNumber],numberMinMax[1,300]"  type="text" value="<%=hsFieldValue["Height"]%>" style="width:100px;"  maxlength="10000" name="Height" id="Height" />(单位:cm)</dd></dl><dl><dt>目前所在地</dt><dd><input  class="validate[optional]"  type="text" value="<%=hsFieldValue["CurrentAddress"]%>" style="width:180px;"  maxlength="512" name="CurrentAddress" id="CurrentAddress" /></dd></dl><dl><dt>户囗所在地</dt><dd><textarea style="height:40px;width:500px;"   class="validate[optional]"  id="Census"  name="Census"><%=hsFieldValue["Census"]%></textarea></dd></dl><dl><dt>工作年限</dt><dd><input  class="validate[optional],numberMinMax[0,50],regex[^\d+(\.\d{1,1})?$,小数位数为 1 至 1 之间]"  type="text" value="<%=hsFieldValue["WorkExperience"]%>" style="width:180px;"  maxlength="10000" name="WorkExperience" id="WorkExperience" />年</dd></dl><dl><dt>英语级别</dt><dd><select  id="EnglishLevel" name="EnglishLevel"><option  <%if(hsFieldValue["EnglishLevel"] != null && hsFieldValue["EnglishLevel"].ToString().Contains("0")) {Response.Write("selected='selected'");}%> value="0">无</option><option  <%if(hsFieldValue["EnglishLevel"] != null && hsFieldValue["EnglishLevel"].ToString().Contains("1")) {Response.Write("selected='selected'");}%> value="1">三级</option><option  <%if(hsFieldValue["EnglishLevel"] != null && hsFieldValue["EnglishLevel"].ToString().Contains("2")) {Response.Write("selected='selected'");}%> value="2">四级</option><option  <%if(hsFieldValue["EnglishLevel"] != null && hsFieldValue["EnglishLevel"].ToString().Contains("3")) {Response.Write("selected='selected'");}%> value="3">六级</option><option  <%if(hsFieldValue["EnglishLevel"] != null && hsFieldValue["EnglishLevel"].ToString().Contains("4")) {Response.Write("selected='selected'");}%> value="4">其它</option></select></dd></dl><dl><dt>英语水平</dt><dd><select  id="EnglishState" name="EnglishState"><option  <%if(hsFieldValue["EnglishState"] != null && hsFieldValue["EnglishState"].ToString().Contains("1")) {Response.Write("selected='selected'");}%> value="1">优</option><option  <%if(hsFieldValue["EnglishState"] != null && hsFieldValue["EnglishState"].ToString().Contains("2")) {Response.Write("selected='selected'");}%> value="2">好</option><option  <%if(hsFieldValue["EnglishState"] != null && hsFieldValue["EnglishState"].ToString().Contains("3")) {Response.Write("selected='selected'");}%> value="3">良</option><option  <%if(hsFieldValue["EnglishState"] != null && hsFieldValue["EnglishState"].ToString().Contains("4")) {Response.Write("selected='selected'");}%> value="4">一般</option><option  <%if(hsFieldValue["EnglishState"] != null && hsFieldValue["EnglishState"].ToString().Contains("5")) {Response.Write("selected='selected'");}%> value="5">差</option></select></dd></dl><dl><dt>第二外语</dt><dd><input  class="validate[optional]"  type="text" value="<%=hsFieldValue["SecondForgin"]%>" style="width:180px;"  maxlength="128" name="SecondForgin" id="SecondForgin" /></dd></dl><dl><dt>水平</dt><dd><select  id="SecondForginLevel" name="SecondForginLevel"><option  <%if(hsFieldValue["SecondForginLevel"] != null && hsFieldValue["SecondForginLevel"].ToString().Contains("1")) {Response.Write("selected='selected'");}%> value="1">优</option><option  <%if(hsFieldValue["SecondForginLevel"] != null && hsFieldValue["SecondForginLevel"].ToString().Contains("2")) {Response.Write("selected='selected'");}%> value="2">好</option><option  <%if(hsFieldValue["SecondForginLevel"] != null && hsFieldValue["SecondForginLevel"].ToString().Contains("3")) {Response.Write("selected='selected'");}%> value="3">良</option><option  <%if(hsFieldValue["SecondForginLevel"] != null && hsFieldValue["SecondForginLevel"].ToString().Contains("4")) {Response.Write("selected='selected'");}%> value="4">一般</option><option  <%if(hsFieldValue["SecondForginLevel"] != null && hsFieldValue["SecondForginLevel"].ToString().Contains("5")) {Response.Write("selected='selected'");}%> value="5">差</option><option  <%if(hsFieldValue["SecondForginLevel"] != null && hsFieldValue["SecondForginLevel"].ToString().Contains("6")) {Response.Write("selected='selected'");}%> value="6">其它</option></select></dd></dl><dl><dt>计算机</dt><dd><select  id="Computer" name="Computer"><option  <%if(hsFieldValue["Computer"] != null && hsFieldValue["Computer"].ToString().Contains("1")) {Response.Write("selected='selected'");}%> value="1">精通</option><option  <%if(hsFieldValue["Computer"] != null && hsFieldValue["Computer"].ToString().Contains("2")) {Response.Write("selected='selected'");}%> value="2">熟练</option><option  <%if(hsFieldValue["Computer"] != null && hsFieldValue["Computer"].ToString().Contains("3")) {Response.Write("selected='selected'");}%> value="3">一般</option><option  <%if(hsFieldValue["Computer"] != null && hsFieldValue["Computer"].ToString().Contains("4")) {Response.Write("selected='selected'");}%> value="4">其它</option></select></dd></dl><dl><dt>最高学历</dt><dd><select  id="HeightestEducate" name="HeightestEducate"><option  <%if(hsFieldValue["HeightestEducate"] != null && hsFieldValue["HeightestEducate"].ToString().Contains("0")) {Response.Write("selected='selected'");}%> value="0">初中</option><option  <%if(hsFieldValue["HeightestEducate"] != null && hsFieldValue["HeightestEducate"].ToString().Contains("1")) {Response.Write("selected='selected'");}%> value="1">高中(中专)</option><option  <%if(hsFieldValue["HeightestEducate"] != null && hsFieldValue["HeightestEducate"].ToString().Contains("2")) {Response.Write("selected='selected'");}%> value="2">大专</option><option  <%if(hsFieldValue["HeightestEducate"] != null && hsFieldValue["HeightestEducate"].ToString().Contains("3")) {Response.Write("selected='selected'");}%> value="3">本科</option><option  <%if(hsFieldValue["HeightestEducate"] != null && hsFieldValue["HeightestEducate"].ToString().Contains("4")) {Response.Write("selected='selected'");}%> value="4">硕士</option><option  <%if(hsFieldValue["HeightestEducate"] != null && hsFieldValue["HeightestEducate"].ToString().Contains("5")) {Response.Write("selected='selected'");}%> value="5">博士</option><option  <%if(hsFieldValue["HeightestEducate"] != null && hsFieldValue["HeightestEducate"].ToString().Contains("6")) {Response.Write("selected='selected'");}%> value="6">博士后